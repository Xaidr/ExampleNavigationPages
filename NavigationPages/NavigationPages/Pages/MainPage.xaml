<Page
    x:Class="NavigationPages.Pages.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:NavigationPages.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <StackPanel
            HorizontalAlignment="Center"
            VerticalAlignment="Center">
            <Button
                x:Name="BtnForward"
                Margin="5"
                Padding="10"
                Content="Forward" />
            <Button
                x:Name="BtnBack"
                Margin="5"
                Padding="10"
                Content="Back" />
        </StackPanel>
    </Grid>
</Page>
